{
  "name": "underthelimits",
  "version": "0.0.0",
  "description": "@TODO",
  "main": "index.js",
  "scripts": {
    "postinstall": "./server/scripts/postinstall.sh",
    "clean": "rm -rf server/db.sqlite3; rm -rf server/scripts/*.sql; rm -rf server/node_modules; rm -rf client/node_modules && rm -rf node_modules",
    "install:qa": "yarn --ignore-scripts --cwd client; yarn --ignore-scripts --cwd server; yarn --ignore-scripts",
    "start:dev": "docker-compose -f docker-compose-dev.yml up --force-recreate --build",
    "start:qa": "yarn --cwd client build:qa && node index.js",
    "start:prod": "docker-compose -f docker-compose.yml up --force-recreate --build",
    "populate:dev": "docker exec -it utl-server-dev npm run populate",
    "populate:qa": "node server/scripts/populate.js",
    "populate:prod": "docker exec -it utl-server npm run populate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:client": "cd client && yarn lint",
    "lint:server": "cd server && yarn lint",
    "lint": "yarn lint:client && yarn lint:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thenkei/UnderTheLimits.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Thenkei/UnderTheLimits/issues"
  },
  "homepage": "https://github.com/Thenkei/UnderTheLimits#readme",
  "devDependencies": {
    "node-cipher": "^6.3.3"
  },
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "sqlite3": "^4.0.6"
  }
}
